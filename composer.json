{
    "name": "jhasheng/purple",
    "type": "library",
    "description": "laravel 5 profiler",
    "keywords": [
        "jhasheng",
        "purple"
    ],
    "homepage": "https://github.com/jhasheng/purple",
    "license": "MIT",
    "authors": [
        {
            "name": "jhasheng",
            "email": "jhasheng@hotmail.com",
            "homepage": "http://www.jiajunjun.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/laravel": "5.0.*|5.1.*|5.2.*",
        "predis/predis": "^1.1",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Purple\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Purple\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
